<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" 
    android:orientation="vertical">
    
     <TextView
        android:id="@+id/tvNombreUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Usuario reserva"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/tvIdReserva"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="IDReserva"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
     <TextView
        android:id="@+id/tvFecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha"
        android:textAppearance="?android:attr/textAppearanceMedium" />
     
      <TextView
        android:id="@+id/tvNombreRestaurante"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre restaurante"
        android:textAppearance="?android:attr/textAppearanceMedium" />
      
       <TextView
        android:id="@+id/tvCarta1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carta1"
        android:textAppearance="?android:attr/textAppearanceMedium" />
       
       <TextView
        android:id="@+id/tvCarta2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carta2"
        android:textAppearance="?android:attr/textAppearanceMedium" />
       
       <TextView
        android:id="@+id/tvOferta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Oferta"
        android:textAppearance="?android:attr/textAppearanceMedium" />
              
      
      <TextView
        android:id="@+id/tvNombreComensal1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre comensal1"
        android:textAppearance="?android:attr/textAppearanceMedium" />
      
      <TextView
        android:id="@+id/tvNombreComensal2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre comensal2"
        android:textAppearance="?android:attr/textAppearanceMedium" />
      
      
   
</LinearLayout>
